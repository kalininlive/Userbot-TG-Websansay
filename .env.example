# Конфигурация API-сервера Userbot TG Websansay

# Порт, на котором запускается API
API_PORT=3000

# API-токен для защиты эндпоинтов
# При установке генерируется случайный hex-ключ и сохраняется в .env
API_TOKEN=your_api_token_here

# Директории для хранения данных
SESSION_DIR=./sessions
UPLOAD_DIR=./uploads
DOWNLOAD_DIR=./downloads
